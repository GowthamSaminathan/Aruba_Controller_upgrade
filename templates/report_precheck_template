precheck_table_data = {{precheck_table}}

$(document).ready(function() {
    $('#precheck_table').DataTable( {
        data: precheck_table_data,
        paging:         false,
        "order": [[ 5, "desc" ]],
        columns: [
            { title: "Device Type" },
            { title: "Hostname" },
            { title: "Host" },
            { title: "Validation" },
            { title: "Result" },
            { title: "Status" },
            { title: "Report type" }

        ]
    } );
} );
